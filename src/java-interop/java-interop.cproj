        <Include>${CombineDir}/LocalJDK/System/Library/Frameworks/JavaVM.framework/Versions/A/Headers</Include>
      </Includes>
    </Includes>
    <ExtraLinkerArguments>-m32 -L /Library/Frameworks/Mono.framework/Libraries -lmonosgen-2.0</ExtraLinkerArguments>
